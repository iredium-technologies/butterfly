{
  "name": "@iredium/butterfly",
  "version": "0.0.4",
  "description": "Express API Framework",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iredium-technologies/butterfly.git"
  },
  "author": "Wahyu Nugraha <nugraha.c.wahyu@gmail.com>",
  "license": "MIT",
  "main": "dist/butterfly.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "backpack dev --inspect=0.0.0.0:9229 --nolazy",
    "build": "BUILD_ENV=true backpack build",
    "start": "node dist/example.js",
    "precommit": "npm run lint",
    "test": "NODE_ENV=test jest",
    "lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore ."
  },
  "keywords": [
    "butterfly",
    "iredium",
    "express",
    "babel",
    "boilerplate",
    "scaffold",
    "es6",
    "es2015",
    "es2016",
    "es2017",
    "jest",
    "eslint"
  ],
  "dependencies": {
    "@types/email-templates": "^3.5.0",
    "@types/mongoose": "^5.3.23",
    "aws-sdk": "^2.221.1",
    "bcrypt": "^3.0.4",
    "body-parser": "^1.18.2",
    "connect-redis": "^3.3.3",
    "cookie-parser": "^1.4.3",
    "cron": "^1.3.0",
    "cross-env": "^5.1.4",
    "dotenv": "^4.0.0",
    "email-templates": "^5.0.4",
    "express": "^4.16.2",
    "firebase-admin": "^5.12.1",
    "hiredis": "^0.5.0",
    "json-loader": "^0.5.7",
    "kue": "^0.11.6",
    "mongoosastic": "^4.4.1",
    "mongoose": "^5.4.19",
    "mongoose-autopopulate": "^0.9.1",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "multer-s3": "^2.7.0",
    "node-uuid": "^1.4.8",
    "pluralize": "^7.0.0",
    "pug": "^2.0.0-beta11",
    "repl-promised": "^0.1.0",
    "request": "^2.82.0",
    "rimraf": "^2.6.2",
    "socket.io": "^2.1.0",
    "socket.io-emitter": "^3.1.1",
    "socket.io-redis": "^5.2.0",
    "source-map-support": "^0.5.4",
    "sticky-session": "^1.1.2",
    "tls": "0.0.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@types/express": "^4.11.1",
    "@types/jest": "^24.0.11",
    "awesome-typescript-loader": "^3.0.0-beta.18",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^22.4.3",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-stage-0": "^6.16.0",
    "backpack-core": "^0.4.3",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-typescript": "^0.12.0",
    "jest": "^21.2.1",
    "supertest": "^3.0.0",
    "ts-loader": "^3.5.0",
    "tslint-config-airbnb": "^5.8.0",
    "typescript": "^2.7.1",
    "typescript-eslint-parser": "^14.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "verbose": true,
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.(ts|tsx)$": "<rootDir>/ts_jest.js"
    },
    "globals": {
      "NODE_ENV": "test"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleDirectories": [
      "node_modules",
      "dist"
    ],
    "moduleNameMapper": {
      "^~/src/(.*)": "<rootDir>/src/$1",
      "^~/example/(.*)": "<rootDir>/example/$1"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$"
  },
  "bugs": {
    "url": "https://github.com/iredium-technologies/iredium/issues"
  },
  "homepage": "https://github.com/iredium-technologies/iredium#readme",
  "directories": {
    "test": "test"
  }
}
